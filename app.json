{
  "expo": {
    "name": "Pocket Eng",
    "slug": "pocket-eng",
    "version": "2.3.2",
    "orientation": "portrait",
    "scheme": "pocketeng",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kmsprod.pocketeng",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kmsprod.pocketeng",
      "versionCode": 1
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-localization",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "93fde987-54de-46a4-91d1-6e346974b3bb"
      }
    },
    "owner": "mishanyawww"
  }
}
