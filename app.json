{
  "expo": {
    "name": "LearningEng",
    "slug": "LearningEng",
    "version": "2.0.6",
    "orientation": "portrait",
    "scheme": "learningeng",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mishanyawww.LearningEng"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4fa5c5cf-ffc3-459f-860b-299a723f2e1b"
      }
    }
  }
}
